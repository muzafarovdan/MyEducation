    "n, m = map(int, input().split())\n",
    "array = list(map(int, input().split()))\n",
    "result = []\n",
    "for i in range(1,m+1):\n",
    "    for j in array:\n",
    "        if i >= j:\n",
    "            i = i - j\n",
    "    result.append(i)\n",
    "print(max(result))"
